using System;
using System.Linq;
using Gibberish.AST;

ironmeta ParseLanguageFile<char, ParseTree> : IronMeta.Matcher.Matcher<char, ParseTree>
{
	File = UseLanguage("fasm") _fasmParser.Declarations:decls -> { return decls; };

	UseLanguage :lang = KW("use language") lang NL;

	Name = /[a-zA-z0-9_.]+/:name -> { return new NameNode(name.Inputs); };

	KW :str = str (WS+ | &NL);
	WS = ' ';
	NL = "\r\n" | "\n" | "\r";
}